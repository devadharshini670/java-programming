import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int original = n;
        int sum = 0;
        while(n>0){
            sum+=n%10;
            n/=10;
        }
        if(original%sum==0){
            System.out.print("Harshad Number");
        }
        else{
            System.out.print("Not Harshad Number");
        }
        
    }
}
